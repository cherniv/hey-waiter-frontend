{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
        {
            "source": "manager.waiter.live",
            "destination": "/manager/"
        },
        {
        "source": "**",
        "destination": "/visitor/"
      } ]
  }
}
