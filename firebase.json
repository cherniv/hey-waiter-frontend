{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
        {
            "source": "/",
            "destination": "/visitor/index.html"
        },
        {
            "source": "/manager/**",
            "destination": "/manager/index.html"
        }
        ]
  }
}
