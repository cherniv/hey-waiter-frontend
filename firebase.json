{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
        "source": "**",
        "destination": "/visitor/index.html"
      },
      {
      "source": "manager.waiter.live",
      "destination": "/manager/"
    } ]
  }
}
